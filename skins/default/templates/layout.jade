head
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>#{packageName} #{packageVersion} API documentation</title>
  <meta name="generator" content="ndoc">
  <meta name="description" content="">
  <meta name="author" content="">

  <meta name="viewport" content="width=device-width,initial-scale=1">

  <!-- CSS concatenated and minified via ant build script-->
  <link rel="stylesheet" href="stylesheets/main.css">
  <!-- end CSS-->

  script(src="javascripts/modernizr-2.0.6.min.js")

body

  //- Navigation
  nav.sidebar
    mixin search()
    mixin menu()

  //- Main content
  article.main

    //-logo
      h1 Logo Here

    //- Readme
    mixin home()
    
    //- Docs
    mixin api()
    
    footer.footer
      p
        <a href="#{src_code_href}">#{packageName} #{packageVersion}</a> API documentation.
      p
        Last updated on #{date}. Generated by <a href="http://github.com/nodeca/ndoc">ndoc</a>


  script(src="javascripts/jquery.js")

  // scripts concatenated and minified via ant build script
  <script defer src="javascripts/plugins.js"></script>
  <script defer src="javascripts/prettify.js"></script>
  <script defer src="javascripts/script.js"></script>
  //-<script defer src="tree.js"></script>
  // end scripts


  //if lt IE 7
    script(src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.3/CFInstall.min.js")
